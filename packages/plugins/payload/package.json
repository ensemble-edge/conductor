{
  "name": "@ensemble-edge/plugin-payload",
  "version": "0.0.1",
  "type": "module",
  "description": "Payload CMS plugin for Conductor",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "conductor",
    "plugin",
    "payload",
    "cms",
    "headless-cms"
  ],
  "author": "Ensemble Edge",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ensemble-edge/conductor",
    "directory": "packages/plugins/payload"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@ensemble-edge/conductor": "^0.4.16"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251121.0",
    "@ensemble-edge/conductor": "workspace:*",
    "typescript": "^5.5.2",
    "vitest": "~3.2.0"
  }
}
