{
  "name": "@conductor/twilio",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Twilio plugin for Conductor - adds SMS and voice messaging capabilities",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "conductor",
    "plugin",
    "twilio",
    "sms",
    "messaging",
    "trigger"
  ],
  "author": "Ensemble Edge",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ensemble-edge/conductor",
    "directory": "packages/plugins/twilio"
  },
  "peerDependencies": {
    "@ensemble-edge/conductor": "^0.3.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251121.0",
    "@ensemble-edge/conductor": "workspace:*",
    "typescript": "^5.5.2",
    "vitest": "~4.0.14",
    "zod": "^3.23.8"
  }
}
