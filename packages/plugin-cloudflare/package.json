{
  "name": "@conductor/cloudflare",
  "version": "0.1.0",
  "type": "module",
  "description": "Cloudflare API plugin for Conductor",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
