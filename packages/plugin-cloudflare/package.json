  "peerDependencies": {
    "@ensemble-edge/conductor": "^0.1.0"
  },
  "devDependencies": {
    "@ensemble-edge/conductor": "workspace:*",
    "@cloudflare/workers-types": "^4.20250110.0",
    "typescript": "^5.5.2",
    "vitest": "~3.2.0"
