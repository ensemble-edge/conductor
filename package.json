{
	"name": "@ensemble-edge/conductor",
	"version": "1.1.0",
	"type": "module",
	"description": "Edge-native orchestration for AI members. Built on Cloudflare Workers.",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		},
		"./cloudflare": {
			"types": "./dist/cloudflare.d.ts",
			"import": "./dist/cloudflare.js",
			"require": "./dist/cloudflare.js"
		},
		"./sdk": {
			"types": "./dist/sdk/index.d.ts",
			"import": "./dist/sdk/index.js",
			"require": "./dist/sdk/index.js"
		},
		"./testing": {
			"types": "./dist/testing/index.d.ts",
			"import": "./dist/testing/index.js",
			"require": "./dist/testing/index.js"
		}
	},
	"bin": {
		"conductor": "./bin/conductor.js"
	},
	"files": [
		"dist",
		"bin",
		"catalog",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"build": "tsc && node scripts/build-cli.js",
		"build:types": "tsc",
		"build:cli": "node scripts/build-cli.js",
		"dev": "wrangler dev",
		"typecheck": "tsc --noEmit",
		"lint": "echo 'Linting... (no eslint config yet)'",
		"lint:fix": "echo 'Linting... (no eslint config yet)'",
		"format": "prettier --write \"src/**/*.ts\"",
		"format:check": "prettier --check \"src/**/*.ts\"",
		"validate": "npm run typecheck && npm run lint && npm run format:check && npm run build && npm test",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"release": "changeset publish",
		"cf-typegen": "wrangler types",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"chalk": "^4.1.2",
		"commander": "^12.1.0",
		"glob": "^10.3.10",
		"handlebars": "^4.7.8",
		"hono": "^4.6.0",
		"liquidjs": "^10.10.0",
		"yaml": "^2.4.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.7",
		"@cloudflare/workers-types": "^4.20241127.0",
		"@types/mjml": "^4.7.4",
		"@types/node": "^20.11.0",
		"@vitest/coverage-v8": "^3.2.4",
		"esbuild": "^0.25.12",
		"prettier": "^3.6.2",
		"typescript": "^5.5.2",
		"vitest": "~3.2.0",
		"wrangler": "^4.45.2"
	},
	"peerDependencies": {
		"@cloudflare/workers-types": "^4.0.0"
	},
	"peerDependenciesMeta": {
		"@ensemble-edge/edgit": {
			"optional": true
		}
	},
	"keywords": [
		"cloudflare",
		"workers",
		"ai",
		"orchestration",
		"edge",
		"llm",
		"workflow",
		"ensemble",
		"conductor"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ensemble-edge/conductor"
	},
	"bugs": {
		"url": "https://github.com/ensemble-edge/conductor/issues"
	},
	"homepage": "https://github.com/ensemble-edge/conductor#readme",
	"author": "Ensemble Edge",
	"license": "Apache-2.0",
	"publishConfig": {
		"access": "public"
	}
}
